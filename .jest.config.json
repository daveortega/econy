{
  "preset": "ts-jest",
  "testEnvironment": "node",
  "coveragePathIgnorePatterns": ["src/generated", "index.ts", "dist"],
  "collectCoverage": true,
  "collectCoverageFrom": ["**/*.ts"],
  "coverageThreshold": {
    "global": {
      "functions": 80,
      "lines": 80,
      "statements": -10
    }
  },
  "maxWorkers": "50%"
}
