{
  "name": "@ecny/migrations",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "private": true,
  "devDependencies": {
    "@ecny/dev-tools": "*"
  },
  "dependencies": {
    "config": "^3.3.12",
    "js-yaml": "^4.1.0",
    "knex": "^3.1.0",
    "pg": "^8.13.0"
  },
  "scripts": {
    "build": "tsc -b .",
    "code:review": "code-review.sh",
    "code:format": "code-review.sh --fix",
    "migrate": "NODE_CONFIG_DIR=../../../config knex --knexfile src/knexfile.ts",
    "migrate:make": "yarn migrate migrate:make",
    "migrate:up": "yarn migrate --debug migrate:up",
    "migrate:test:up": "NODE_ENV=test yarn migrate --debug migrate:up",
    "migrate:latest": "yarn migrate --debug migrate:latest",
    "migrate:test:latest": "NODE_ENV=test yarn migrate --debug migrate:latest",
    "migrate:down": "yarn migrate --debug migrate:down",
    "migrate:test:down": "NODE_ENV=test yarn migrate --debug migrate:down",
    "migrate:rollback": "yarn migrate --debug migrate:rollback",
    "migrate:test:rollback": "NODE_ENV=test yarn migrate --debug migrate:rollback",
    "test:unit": "echo skipped",
    "test:coverage": "coverage.sh",
    "unused:artifacts": "unused-artifacts.sh"
  }
}
